<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.messangerapplication">

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SendMoneyActivity" />
        <activity android:name=".RequestActivity"/>
        <activity android:name=".RequestChoiceActivity"
            android:theme="@style/AppTheme.PopMe"/>
        <activity
            android:name=".ShareActivity"
            android:theme="@style/AppTheme.PopMe" />
        <activity
            android:name=".AddChannel"
            android:theme="@style/AppTheme.PopMe" />
        <activity android:name=".Messages" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LogRegActivity" />
        <activity android:name=".Notes" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".ChangeNameActivity" />
        <activity android:name=".WalletActivity" />

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-2830062687302437~7300093995" />
    </application>

</manifest>